{
  "name": "@stropitek/gatsby-plugin-intl",
  "description": "Gatsby plugin to add react-intl onto a site",
  "version": "0.4.1",
  "author": "Daewoong Moon <wiziple@gmail.com>",
  "bugs": {
    "url": "https://github.com/wiziple/gatsby-plugin-intl"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "browser-lang": "^0.1.0",
    "intl": "^1.2.5",
    "react-intl": "^5.10.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "babel-preset-gatsby-package": "^0.11.0",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  },
  "homepage": "https://github.com/wiziple/gatsby-plugin-intl",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-intl",
    "i18n",
    "intl",
    "react-intl"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^2.31.1"
  },
  "repository": "https://github.com/wiziple/gatsby-plugin-intl",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "format": "prettier --write {src,__tests__}/**/*.js",
    "test": "jest",
    "clean": "rimraf *.js"
  }
}
